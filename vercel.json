{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.1" }
    },
    "routes": [ 
        { "src": "/build/(.*)", "dest": "/public/build/$1" },
        { "src": "/sw.js", "dest": "/public/sw.js" },
        { "src": "/manifest.webmanifest", "dest": "/public/manifest.webmanifest" },
        { "src": "/logo.png", "dest": "/public/logo.png" },
        { "src": "/logo.svg", "dest": "/public/logo.svg" },
        { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://dompet-pintar-gamma.vercel.app",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "cookie"
    }
}